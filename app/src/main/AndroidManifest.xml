<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coffeeorderingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Coffee"></activity>
        <activity android:name=".CoffeeActivity"></activity>
        <activity android:name=".CoffeeCategoryActivity"></activity>
        <activity android:name=".TeaCategoryActivity"></activity>
        <activity android:name=".TeaActivity"></activity>
        <activity android:name=".Tea"></activity>
    </application>

</manifest>